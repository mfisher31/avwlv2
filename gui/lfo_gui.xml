<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkAdjustment" id="adjustmentFrequency">
    <property name="upper">100</property>
    <property name="value">5</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjustmentPhi0">
    <property name="upper">6.2830000000000004</property>
    <property name="page_increment">100</property>
  </object>
  <object class="GtkWindow" id="lfo_window">
    <property name="can_focus">False</property>
    <property name="type">popup</property>
    <property name="title" translatable="yes">VCO2</property>
    <property name="resizable">False</property>
    <property name="destroy_with_parent">True</property>
    <property name="has_resize_grip">False</property>
    <child>
      <object class="GtkVBox" id="boxFreq">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="homogeneous">True</property>
        <child>
          <object class="GtkLabel" id="labelWaveForm">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Wave Form</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBoxText" id="comboboxtextWaveForm">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <items>
              <item translatable="yes">Sine</item>
              <item translatable="yes">Triangle</item>
              <item translatable="yes">Sawtooth Up</item>
              <item translatable="yes">Sawtooth Down</item>
              <item translatable="yes">Rectangle</item>
              <item translatable="yes">S &amp; H</item>
            </items>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="labelFrequency">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Frequency</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkHScale" id="scaleFrequency">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="adjustment">adjustmentFrequency</property>
            <property name="round_digits">3</property>
            <property name="digits">3</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">3</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="labelPhi0">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Phi0</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">4</property>
          </packing>
        </child>
        <child>
          <object class="GtkHScale" id="scalePhi0">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="adjustment">adjustmentPhi0</property>
            <property name="round_digits">0</property>
            <property name="digits">0</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">5</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>

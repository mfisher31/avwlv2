@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix cv: <http://lv2plug.in/ns/ext/cv-port#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://avwlv2.sourceforge.net/plugins/avw/vcpanning/gui>
  a guiext:GtkUI ;
  guiext:binary <vcpanning_gui.so> ;
  guiext:requiredFeature guiext:makeResident .

<http://avwlv2.sourceforge.net/plugins/avw/vcpanning>
	a lv2:Plugin ,
		lv2:VCPANNING ;
	doap:maintainer [
		foaf:name "Aur√©lien Leblond" ;
		foaf:mbox <mailto:blablack@gmail.com>
	] ;
	doap:name "AMS VC Panning" ;
	doap:license <http://opensource.org/licenses/isc-license> ;
	ll:pegName "p" ;
	guiext:ui <http://avwlv2.sourceforge.net/plugins/avw/vcpanning/gui> ;
	lv2:property lv2:hardRtCapable ;
	lv2:port [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In"
	] ,	[
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "panCV" ;
		lv2:name "Pan CV" 
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "panOffset" ;
		lv2:name "Pan Offset" ;
		lv2:default 0 ;
		lv2:minimum -1 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "panGain" ;
		lv2:name "Pan Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 2 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "panningMode" ;
		lv2:name "Panning Mode" ;
		lv2:default 0;
		lv2:minimum 0;
		lv2:maximum 4;
		lv2:scalePoint  
		[
		  		rdfs:label "VC Control" ;
		    	rdf:value 0
		] ,	[
				rdfs:label "Fixed Alternating Panorama - Full Width" ;
				rdf:value 1
		] ,	[
		  		rdfs:label "Fixed Alternating Panorama - Half Width" ;
				rdf:value 2
		] ,	[
		  		rdfs:label "Fixed Alternating Panorama - Quarter Width" ;
				rdf:value 3
		], 	[
		  		rdfs:label "Mono" ;
				rdf:value 4
		]
	] ,	[
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 5 ;
		lv2:symbol "out1" ;
		lv2:name "Out 1"
	] ,	[
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 6 ;
		lv2:symbol "out2" ;
		lv2:name "Out 2"
	] .

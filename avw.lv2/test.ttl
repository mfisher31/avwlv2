@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#> .

<http://avwlv2.sourceforge.net/plugins/avw/test>
	a lv2:Plugin ,
		lv2:TEST ;
	doap:maintainer [
		foaf:name "Aurelien Leblond" ;
		foaf:mbox <mailto:blablack@gmail.com>
	] ;
	doap:name "AMS TEST" ;
	doap:license <http://opensource.org/licenses/isc-license> ;
	ll:pegName "p" ;
	lv2:property lv2:hardRtCapable ;
	lv2:port [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "freq" ;
		lv2:name "Port - Freq" 
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "octave" ;
		lv2:name "Frequency - Octave" ; 
		lv2:default 3 ;
		lv2:minimum 0 ;
		lv2:maximum 6
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "tune" ;
		lv2:name "Frequency - Tune" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "harmonic" ;
		lv2:name "Frequency - Harmonic" ;
		lv2:default 1 ;
		lv2:minimum 1 ;
		lv2:maximum 16
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "subharmonic" ;
		lv2:name "Frequency - Subharmonic" ;
		lv2:default 1 ;
		lv2:minimum 1 ;
		lv2:maximum 16
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "pw0" ;
		lv2:name "Pulse Width / Phase - PW0" ;
		lv2:default 0.5 ;
		lv2:minimum 0.1 ;
		lv2:maximum 0.9
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "pwGain" ;
		lv2:name "Pulse Width / Phase - PW Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "phi0" ;
		lv2:name "Pulse Width / Phase - Phi0" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 6.283
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "expFMGain" ;
		lv2:name "Modulation / Aux WaveForm - Exp. FM Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 10
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 9 ;
		lv2:symbol "linFMGain" ;
		lv2:name "Modulation / Aux WaveForm - Lin. FM Gain" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 10
	] ,	[
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 10 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	] .

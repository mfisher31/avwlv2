@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#> .

<http://avwlv2.sourceforge.net/plugins/avw/lfo>
	a lv2:Plugin, lv2:LFO ;
	doap:maintainer 
	[
		foaf:name "Aur√©lien Leblond" ;
		foaf:mbox <mailto:blablack@gmail.com>
	] ;
	doap:name "AMS LFO" ;
	doap:license <http://opensource.org/licenses/isc-license> ;
	ll:pegName "p" ;
	lv2:property lv2:hardRtCapable ;
	lv2:port [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "reset" ;
		lv2:name "Reset" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "frequency" ;
		lv2:name "Frequency" ;
		lv2:default 5.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 100.0 
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "phi0" ;
		lv2:name "Phi0" ;
		lv2:default 0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 6.28
	] ,	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "sine" ;
		lv2:name "Sine" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	] ,	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 4 ;
		lv2:symbol "triangle" ;
		lv2:name "Triangle" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	],	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 5 ;
		lv2:symbol "sawUp" ;
		lv2:name "Saw Up" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	],	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 6 ;
		lv2:symbol "sawDown" ;
		lv2:name "Saw Down" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0 
	],	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 7 ;
		lv2:symbol "rectangle" ;
		lv2:name "Rectangle" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	],	[
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 8 ;
		lv2:symbol "sampleAndHold" ;
		lv2:name "S & H" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0
	] .

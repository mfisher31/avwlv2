@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#> .

<http://avwlv2.sourceforge.net/plugins/avw/delay>
	a lv2:Plugin ,
		lv2:DELAY ;
	doap:maintainer [
		foaf:name "Aur√©lien Leblond" ;
		foaf:mbox <mailto:blablack@gmail.com>
	] ;
	doap:name "AMS Delay" ;
	doap:license <http://opensource.org/licenses/isc-license> ;
	ll:pegName "p" ;
	lv2:property lv2:hardRtCapable ;
	lv2:port [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "delay" ;
		lv2:name "Delay" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 10 
	] ,	[
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "input" ;
		lv2:name "Input"
	] ,	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "output" ;
		lv2:name "Output"
	].
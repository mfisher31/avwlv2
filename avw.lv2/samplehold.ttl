@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .

<http://avwlv2.sourceforge.net/plugins/avw/samplehold>
	a lv2:Plugin ,
		lv2:DelayPlugin ;
	lv2:project <http://avwlv2.sourceforge.net/plugins/avw/> ;        
	doap:name "AMS Sample&Hold" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "triggerlevel" ;
		lv2:name "Trigger Level" ;
		lv2:default 0.5 ;
		lv2:minimum 0 ;
		lv2:maximum 10 
	] ,	[
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "input" ;
		lv2:name "Input"
	] ,	[
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 2 ;
		lv2:symbol "trigger" ;
		lv2:name "Trigger"
	] ,	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "output" ;
		lv2:name "Output"
	] ,	[
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 4 ;
		lv2:symbol "gate" ;
		lv2:name "Gate"
	].

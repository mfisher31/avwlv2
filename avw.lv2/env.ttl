@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://avwlv2.sourceforge.net/plugins/avw/env/gui>
  	a guiext:GtkUI ;
  	guiext:binary <env_gui.so> .

<http://avwlv2.sourceforge.net/plugins/avw/env>
	a lv2:Plugin ,
		lv2:EnvelopePlugin ;
	lv2:project <http://avwlv2.sourceforge.net/plugins/avw/> ;        
	doap:name "AMS Envelope" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://avwlv2.sourceforge.net/plugins/avw/env/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "gate" ;
		lv2:name "Gate" 
	] ,	[
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "retrigger" ;
		lv2:name "Retrigger"  
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "attack" ;
		lv2:name "Attack" ; 
		lv2:default 0.05 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "decay" ;
		lv2:name "Decay" ;
		lv2:default 0.1 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] , [
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "sustain" ;
		lv2:name "Sustain" ;
		lv2:default 0.7 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "release" ;
		lv2:name "Release" ;
		lv2:default 0.05 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "delay" ;
		lv2:name "Delay" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "hold" ;
		lv2:name "Hold" ;
		lv2:default 0.02 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	] ,	[
	  	a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "timeScale" ;
		lv2:name "Time Scale" ;
		lv2:default 0.1 ;
		lv2:minimum 1 ;
		lv2:maximum 10 ;
	] ,	[
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 9 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	] ,	[
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 10 ;
		lv2:symbol "invOut" ;
		lv2:name "Inverse Out"
	].

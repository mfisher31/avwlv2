@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .
@prefix atom: 	<http://lv2plug.in/ns/ext/atom#> .
@prefix urid: 	<http://lv2plug.in/ns/ext/urid#> .
@prefix time: 	<http://lv2plug.in/ns/ext/time#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://avwlv2.sourceforge.net/plugins/avw/beatslicer_mono/gui>
  a guiext:GtkUI ;
  guiext:binary <beatslicer_mono_gui.so> .

<http://avwlv2.sourceforge.net/plugins/avw/beatslicer_mono>
	a lv2:Plugin ;
	lv2:project <http://avwlv2.sourceforge.net/plugins/avw/> ;        
	doap:name "BeatSlicer - Mono" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://avwlv2.sourceforge.net/plugins/avw/beatslicer_mono/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port  [
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 0 ;
		lv2:symbol "trigger" ;
		lv2:name "Trigger" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort , atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports time:Position ;
		lv2:index 1 ;
		lv2:symbol "control" ;
		lv2:name "Control" ;
	] ,	[
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 2 ;
		lv2:symbol "input" ;
		lv2:name "Input"
	] ,	[
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 3 ;
		lv2:symbol "output" ;
		lv2:name "Output"
	] ,	[
		a lv2:InputPort , lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "slice" ;
		lv2:name "Size of Slice" ;
		lv2:default 1.0 ;
		lv2:minimum 0.125 ;
		lv2:maximum 1.0 ;
	] ,	[
		a lv2:InputPort , lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "reverse" ;
		lv2:name "Reverse" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:portProperty pprops:hasStrictBounds ;
		lv2:portProperty lv2:toggled ;
	] .

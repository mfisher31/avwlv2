<http://avwlv2.sourceforge.net/plugins/avw/mousexy>
	a lv2:Plugin ,
		lv2:MOUSEXY ;
	doap:maintainer [
		foaf:name "Aur√©lien Leblond" ;
		foaf:mbox <mailto:blablack@gmail.com>
	] ;
	doap:name "Mouse XY" ;
	doap:license <http://opensource.org/licenses/isc-license> ;
	ll:pegName "p" ;
	lv2:property lv2:hardRtCapable ;
	lv2:port [
